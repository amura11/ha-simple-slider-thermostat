    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch Firefox",
        "type": "firefox",
        "request": "launch",
        "reAttach": true,
        "url": "http://localhost:8123",
        "webRoot": "${workspaceFolder}/dist",
        "preLaunchTask": "start-debug",
        "postDebugTask": "stop-debug",
        "skipFiles": ["**/frontend_latest/**"]
      }
    ]
  }